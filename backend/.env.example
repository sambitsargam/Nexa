// backend/.env.example
# 3xpl API Configuration
THREEPL_API_BASE=https://sandbox-api.3xpl.com
THREEPL_API_RATE_LIMIT=100

# CoFHE Configuration (Fhenix)
COFHE_API_BASE=https://api.fhenix.zone
COFHE_API_KEY=your_cofhe_api_key
COFHE_PRIVATE_KEY=your_cofhe_private_key

# nilDB & SecretVaults Configuration
NILDB_API_BASE=https://api.nillion.com/nildb
NILDB_API_KEY=your_nildb_api_key
NILDB_USER_ID=your_user_id
NILDB_VAULT_ID=your_vault_id

# nilAI Configuration
NILAI_API_BASE=https://api.nillion.com/nilai
NILAI_API_KEY=your_nilai_api_key
NILAI_MODEL=gpt-4

# Server Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=debug

# Demo/Dev Mode
DEV_MODE=true
ENABLE_DEMO_DECRYPT=true
